load("@io_bazel_rules_go//go:def.bzl", "go_library", "go_test")

go_library(
    name = "go_default_library",
    srcs = [
        "collelem.go",
        "colltab.go",
        "contract.go",
        "iter.go",
        "numeric.go",
        "table.go",
        "trie.go",
